{
  "name": "@fabrix/spool-sitemap",
  "version": "1.1.1",
  "description": "Sitemap - Spool for Fabrix to generate Sitemaps through a common API",
  "homepage": "https://fabrix.app",
  "author": {
    "name": "scott-wyatt",
    "email": "scottwyatt86@gmail.com",
    "url": "https://cali-style.com"
  },
  "keywords": [
    "sitemap",
    "spool",
    "fabrix",
    "trailjs"
  ],
  "scripts": {
    "build": "tsc -p ./lib/tsconfig.release.json",
    "lint": "tslint -p ./lib",
    "watch": "tsc -w -p ./lib/tsconfig.release.json",
    "test": "npm run clean && npm run lint && npm run build && nyc mocha",
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && npm run build",
    "clean": "rm -rf dist",
    "ci": "cd .. && ci"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "pre-commit": [
    "test"
  ],
  "dependencies": {
    "joi": "^13.5.2",
    "sitemap": "^1.13.0"
  },
  "devDependencies": {
    "@fabrix/fabrix": "^1.1.4",
    "@fabrix/lint": "^1.0.0-alpha.3",
    "@fabrix/spool-i18n": "^1.1.0",
    "@fabrix/spool-sequelize": "^1.1.4",
    "@fabrix/spool-express": "^1.1.4",
    "@fabrix/spool-engine": "^1.1.3",
    "@fabrix/spool-router": "^1.1.3",
    "@fabrix/spool-caches": "^1.1.0",
    "cache-manager-fs": "^1.0.6",
    "express": "^4.15.2",
    "lodash": "^4.17.4",
    "mocha": "^5",
    "nyc": "^12.0.2",
    "pg": "^6.4.2",
    "smokesignals": "^2.0.0",
    "supertest": "^2.0.1",
    "tslib": "~1.9.0",
    "tslint": "~5.10.0",
    "tslint-microsoft-contrib": "~5.0.3",
    "tsutils": "~2.27.1",
    "typescript": "~2.8.1"
  },
  "repository": "git@github.com:fabrix-app/spool-sitemap.git",
  "license": "MIT",
  "engines": {
    "node": ">= 7.6.0 =< 10.0.0",
    "npm": ">= 3.10.0"
  }
}
